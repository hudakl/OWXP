<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">

<service-builder package-path="com.liferay.micro.maintainance.task">
	<namespace>Task</namespace>
	<entity name="TaskEntry" uuid="true" local-service="true" remote-service="true" trash-enabled="false">

		<!-- PK fields -->

		<column name="taskId" type="long" primary="true" />

		<!-- Audit fields -->

		<column name="createDate" type="Date" />

		<!-- Other fields -->

		<column name="taskName" type="String" />

		<!-- Order -->

		<order by="asc">
			<order-column name="taskName" />
		</order>

		<!-- Finder methods -->

		<finder name="TasksById" return-type="TaskEntry">
			<finder-column name="taskId" />
		</finder>
		<finder name="TasksByName" return-type="TaskEntry">
			<finder-column name="taskName" />
		</finder>

	</entity>

	<entity name="CandidateMaintenance" uuid="true" local-service="true" remote-service="true" trash-enabled="false">

		<!-- PK fields -->

		<column name="candidateMaintenanceId" type="long" primary="true" />

		<!-- Other fields -->

		<column name="candidateId" type="long" />
		<column name="taskId" type="long" />
		<column name="createDate" type="Date" />

		<!-- Finder methods -->

		<finder name="CandidateIds" return-type="Collection">
			<finder-column name="candidateId" />
		</finder>
		<finder name="TaskIds" return-type="Collection">
			<finder-column name="taskId" />
		</finder>
		<finder name="C_T" return-type="CandidateMaintenance">
			<finder-column name="candidateId" />
			<finder-column name="taskId" />
		</finder>
	</entity>

	<exceptions>
	</exceptions>
</service-builder>